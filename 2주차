#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int n;
    int id, idl;
    int a;
    int b;
    printf("상품 종류 수 입력 : ");
    scanf("%d", n);
    for(int i=0; i<n; i++){
        idl = id[i]+1;
    }
    printf("상품별 입고 수량 입력 : ");
    for(int l = 0; l<id; l++){
        scanf("%d", a);
    }
    printf("상품별 판매 수량 입력 : ");
    for(int j = 0; j<id; j++){
        scanf("%d", b);
    }
    scanf("%d", b);
}
